<div class="row">

</div>

<div class="row">

  <div class="col-md-8">

    <% Professor.read.order("plast_name asc").each do |professor| %>

      <div class="row professor">

        <div class="col-md-4">
          <%= link_to image_tag(professor["photo_url"], "height" => 250, "width" => 200), "/professors/#{professor["id"]}" %>
        </div>

        <div class="col-md-8">
          <h3> <%= professor["pfirst_name"] %> <%= link_to professor["plast_name"], "/professors/#{professor["id"]}" %> </h3>


    </div>
</div>

<% end %>
